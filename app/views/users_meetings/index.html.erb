<p id="notice"><%= notice %></p>

<h1>Listing Users Meetings</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Meeting</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users_meetings.each do |users_meeting| %>
      <tr>
        <td><%= users_meeting.user_id %></td>
        <td><%= users_meeting.meeting_id %></td>
        <td><%= link_to 'Show', users_meeting %></td>
        <td><%= link_to 'Edit', edit_users_meeting_path(users_meeting) %></td>
        <td><%= link_to 'Destroy', users_meeting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Users meeting', new_users_meeting_path %>
